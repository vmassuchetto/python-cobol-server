#: api.py:76
msgid "Common options"
msgstr ""

#: api.py:78
msgid "print this help message"
msgstr "imprime esta mensagem de ajuda"

#: api.py:79
#, python-format
msgid "operation name: %s"
msgstr "nome da operação: %s"

#: api.py:80 api.py:81 api.py:82 api.py:83
msgid "(required)"
msgstr "(obrigatorio)"

#: api.py:81
msgid "path to the copybook file"
msgstr "caminho para o arquivo copybook"

#: api.py:82
msgid "path to the datafile"
msgstr "caminho para o arquivo de dados"

#: api.py:83
msgid "comma separated list of keys"
msgstr "lista de chaves separada por virgulas"

#: api.py:84
msgid "put the main key first and the alternate keys last"
msgstr "coloque a chave principal antes e as chaves alternadas depois"

#: api.py:85
msgid "key to use in operation"
msgstr "chave para usar nesta operacao"

#: api.py:86
msgid "value to match the key"
msgstr "valor para comparar com a chave"

#: api.py:87
msgid "input JSON object for the operation"
msgstr "objeto JSON de entrada para a operacao"

#: api.py:88 api.py:94
msgid "check the README.md file for examples"
msgstr "verifique o arquivo README.md para exemplos"

#: api.py:90
msgid "Partial search options"
msgstr "Opcoes para busca parcial"

#: api.py:92
msgid "super key in case of doing a partial search"
msgstr "superchave em caso de busca parcial"

#: api.py:93
msgid "part of --keyname in the --superkeyname as a [start:end] format"
msgstr "parte de --keyname em --superkeyname no formato [inicio:fim]"

#: api.py:95
msgid "of how to use partial key searches"
msgstr "de como usar buscas parciais"

#: api.py:116
msgid "ERROR: No arguments provided."
msgstr "ERRO: Nenhum argumento fornecido."

